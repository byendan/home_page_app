
<ul class="nav nav-tabs" role="tablist">
	<% if @page_data != nil %>
	<% @page_data.each do |s, t| %>
	<% @name_no_space = s.name.gsub("%20", "").gsub(" ", "") %>
	<li role="presentation" id="tabSheet">
		<a href='#<%= @name_no_space  %>' id="<%= s.id %>"  aria-controls='<%= @name_no_space %>' role="tab" data-toggle="tab"><%= s.name %></a>
	</li>
	<% end %>
	<% end %>
	<li role="presentation" id="tabSheet">
		<a href='#add' aria-controls="add" role="tab" data-toggle="tab"><span class="glyphicon glyphicon-plus"></span></a>
	</li>
</ul>



<div class="tab-content">
	<% if @page_data != nil%>
	<% @page_data.each do |s, t| %>
	<div role="tabpanel" class="tab-pane" id='<%= s.name.gsub("%20", "").gsub(" ", "") %>' >

		<div class="row tabs-area">
			<div class="col-md-12 sheet-col" id='<%= s.id %>'>
				<% unless s.tabs == nil %>
				<% s.tabs.each do |tab| %>
				<%= render tab if tab != nil %>

				<% end %>
				<% end %>
			</div>
		</div>
		


	</div>
	<% end %>
	<% end %>
	<div role="tabpanel" class="tab-pane" id="add">
		<%= render 'sheets/sheet_form' %>
	</div>
</div>



