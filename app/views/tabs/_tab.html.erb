<%= stylesheet_link_tag "def_home_box" %>
<% if tab.name != nil %>
	<a href="https://<%= tab.address %>" target="_blank">
		<div class="box">
			<div class="box-inner">
				<div class="box-pic">
					<%= link_to "delete", tab, method: :delete,
											   data: {confirm: "You sure?" } %>
					<%= image_tag tab.picture.url if tab.picture? %>
				</div>
				<div class="box-title center">
					<%= tab.name %>
				</div>	
			</div>
		</div>
	</a>
<% end %>
