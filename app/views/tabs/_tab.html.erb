
<% if tab.name != nil %>


	<a style="display:block" href="https://<%= tab.address %>" target="_blank">
		<div class="box" >
			<div class="box-inner">
				<div class="box-pic center">
					
					<%= image_tag tab.picture.url if tab.picture? %>
				</div>
				<div class="box-title center">
					<%= tab.name %>
				</div>	
			</div>
		</div>
	</a>

<% end %>


	<% if false %>
  
  <%= link_to "delete", tab, method: :delete,
											   data: {confirm: "You sure?" } %>
	
  
	<% end %>
	
	

